pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }

        stage('Display Sap id') {
            steps {
                sh 'echo "Sapid"'
            }
        }

        stage('Display enrollment number') {
            steps {
                sh 'echo "enrollmentnumber"'
            }
        }
    }
}
